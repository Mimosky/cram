<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:HTMLPanel ui:field="main">
		<b:Row>
			<b:Column offset="1" size="2">
				<b:Button ui:field="previousButton">Précédent</b:Button>
				<b:Button ui:field="nextButton">Suivant</b:Button>
			</b:Column>
			<b:Column offset="3" size="4">
				<b:ListBox ui:field="listBox" />
				<b:Button ui:field="display">Afficher</b:Button>
			</b:Column>
		</b:Row>
		<b:Column  size="10">
			<g:HTMLPanel ui:field="calendarPanel" />
		</b:Column>
	</g:HTMLPanel>
</ui:UiBinder>